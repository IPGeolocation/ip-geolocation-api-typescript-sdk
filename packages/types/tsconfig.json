{
  "compilerOptions": {
    "declaration": true,
    "target": "ES6",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "dist",
    "rootDir": "../..",
    "moduleResolution": "node",
    "typeRoots": [
      "../../node_modules/@types"
    ]
  },
  "include": [
    "../../models.ts",
    "../../models/**/*.ts"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
